import java.util.Arrays;
import java.util.Scanner;

public class ElemanSiralama {
    public static void main(String[] args) {
    siralanmisArray();
    }

    static void siralanmisArray() {
        Scanner input = new Scanner(System.in);
        System.out.println("Array uzunlugunu belirtiniz!");

        int arrayUzunlugu = input.nextInt();

        int[] list = new int[arrayUzunlugu];

        for (int i = 0; i < arrayUzunlugu; i++) {
            list[i] = input.nextInt();
        }

        System.out.println("Onceki indisli Array " + Arrays.toString(list));
        Arrays.sort(list);
        System.out.println("Onceki indisli Array " + Arrays.toString(list));

    }
}
